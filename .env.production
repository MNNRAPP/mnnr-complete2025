# Railway Production Environment Variables
# Set these in Railway Dashboard: Settings > Variables

# Core Application
NODE_ENV=production
NEXT_PUBLIC_SITE_NAME=MNNR
NEXT_PUBLIC_SITE_URL=https://mnnr-production.up.railway.app
NEXT_PUBLIC_RP_ID=mnnr-production.up.railway.app

# Security Features
NEXT_PUBLIC_ENABLE_PASSKEYS=true
NEXT_PUBLIC_ENABLE_MFA=true
NEXT_PUBLIC_ENABLE_AUDIT_LOGGING=true
NEXT_PUBLIC_ENABLE_E2EE=true

# Supabase (ADD YOUR VALUES)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Stripe (ADD YOUR VALUES)
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Redis/Upstash (ADD YOUR VALUES)
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here

# Sentry (ADD YOUR VALUES)
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# OAuth (Optional)
SUPABASE_AUTH_EXTERNAL_GITHUB_REDIRECT_URI=https://mnnr-production.up.railway.app/auth/callback
SUPABASE_AUTH_EXTERNAL_GITHUB_CLIENT_ID=
SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=